{% extends 'base.html.twig' %}

{% block title %}Predictive{% endblock %}

{% block body %}
    <h1>Predictive</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ predictive.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ predictive.name }}</td>
            </tr>
            <tr>
                <th>Added_date</th>
                <td>{{ predictive.addedDate }}</td>
            </tr>
            <tr>
                <th>Added_time</th>
                <td>{{ predictive.addedTime }}</td>
            </tr>
            <tr>
                <th>Status</th>
                <td>{{ predictive.status }}</td>
            </tr>
            <tr>
                <th>Status_percent</th>
                <td>{{ predictive.statusPercent }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_predictive_index') }}">back to list</a>

    <a href="{{ path('app_predictive_edit', {'id': predictive.id}) }}">edit</a>

    {{ include('predictive/_delete_form.html.twig') }}
{% endblock %}
